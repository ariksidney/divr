{
  "name": "divr",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "config": "ts-node -O '{\"module\": \"commonjs\"}' ./config.index.ts",
    "build": "npm run config && ng build --prod",
    "deploy": "firebase deploy --only hosting",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/divr/stats.json",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.22",
    "@angular/animations": "^12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/fire": "^7.0",
    "@angular/forms": "~12.2.2",
    "@angular/google-maps": "^12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@angular/service-worker": "~12.2.2",
    "core-js": "^2.5.4",
    "firebase": "^9.4.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/language-service": "~12.2.2",
    "@types/google.maps": "^3.45.4",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "ts-node": "~7.0.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
